<div id="content" class="template-{{view}}">
	<div class="row">
		<section id="park-info">
			{{#if flickr.total}}
				{{>block-photo-carousel}}
			{{/if}}
			<p class="site-name"><a href="/">{{#__}}Caliparks{{/__}}</a></p>
			<div class="park-details">
				<p class="agency-name"><a href="#">{{cpadPark.mng_agncy}}</a></p>
				{{>block-park-name}}
				<p class="social-breakdown">
					{{#if twitter}}
						<span class="total{{twitter.total}}">{{#pluralize}}{{twitter.total}} <span>{{#__}}Tweet{{/__}}</span>|<span>{{#__}}Tweets{{/__}}</span>{{/pluralize}}
						&nbsp;&nbsp;</span>
					{{/if}}
					{{#if total_any_photos}}
						<span class="total{{total_any_photos}}">{{#pluralize}}{{total_any_photos}} {{#__}}<span>Photo</span>{{/__}}|{{#__}}<span>Photos</span>{{/__}}{{/pluralize}}
						&nbsp;&nbsp;</span>
					{{/if}}
					{{#if foursquare.venues_count}}
						<span class="total{{foursquare.venues_count}}">{{#pluralize}}{{foursquare.venues_count}} {{#__}}<span>Venue</span>{{/__}}|{{#__}}<span>Venues</span>{{/__}}{{/pluralize}}
						&nbsp;&nbsp;</span>
					{{/if}}
					{{#if foursquare.venues_checkins}}
						<span class="total{{foursquare.venues_checkins}}">{{#pluralize}}{{foursquare.venues_checkins}} {{#__}}<span>Check-in</span>{{/__}}|{{#__}}<span>Check-ins</span>{{/__}}{{/pluralize}}
						&nbsp;&nbsp;</span>
					{{/if}}
					{{#if foursquare.venues_tips}}
						<span class="total{{foursquare.venues_tips}}">{{#pluralize}}{{foursquare.venues_tips}} {{#__}}<span>Tip</span>{{/__}}|{{#__}}<span>Tips</span>{{/__}}{{/pluralize}}
						&nbsp;&nbsp;</span>
					{{/if}}
				</p>
				<div>
					<button class="action" onclick="(function(){console.log(viewData);location.href='{{cpadPark.park_url}}';}());">Park Info</button>
					<button class="action" onclick="(function(){alert('The camp here feature is not yet available.');}());">Camp Here</button>
					<div class="fb-like" data-href="https://developers.facebook.com/docs/plugins/" data-layout="button" data-action="like" data-show-faces="false" data-share="false" data-href="http://preview.parks.stamen.com/park/{{park_id}}"></div>
				</div>
			</div>
		</section>
		<section id="maps">
			{{> block-static-park-map}}
		</section>
	</div>
	{{#if instagram.total}}
	<div class="row">
		{{>block-instagram-strip}}
	</div>
	{{/if}}
	<div class="row">
		<section id="get-out">
			<h2>{{#__}}Get outside and...{{/__}}</h2>
			{{> block-stories-flexy}}
		</section>
	</div>
	<div class="row">
		<section id="search-area">
			<label>{{#__}}Get outside and...{{/__}}</label>
			{{>block-search-box}}
		</section>
	</div>
</div>
<script>
	var viewData = {
		"centroid"  : {{{centroid}}},
		"bbox"      : {{{bbox}}},
		"unit_name" : "{{unit_name}}"
	}
</script>

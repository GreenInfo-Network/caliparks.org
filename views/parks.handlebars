<h1>{{appTitle}}</h1>

<div style="text-align:center;margin-top:20px;">
{{#each parks}}
<style>
  #glop-park-{{su_id}} {
    background-image:url('http://stamen-postgis-raster-server.herokuapp.com/cpad/{{su_id}}-888462-200-200.png');
  }

  #glop-park-{{su_id}}:hover {
    background-image:url('http://stamen-postgis-raster-server.herokuapp.com/cpad/{{su_id}}-85BBE1-200-200.png');
  }
</style>
<div style="display:inline-block;margin:10px;width:200px;height:220px;">
  <a href="/park/{{su_id}}"><div id="glop-park-{{su_id}}" class="glop-park" style="width:200px;height:200px;display:inline-block;background-position: initial;" data-park-id="{{su_id}}"></div></a>
  <h3 style="text-overflow:ellipsis;white-space:nowrap;width:200px;overflow:hidden;"><a href="/park/{{su_id}}">{{unit_name}}</a></h3>
</div>
{{/each}}
</div>
{
  "name": "parks.stamen.com_app",
  "private": true,
  "version": "0.1.0",
  "author": "Sean Connelley connelley@stamen.com",
  "description": "A nice project about parks",
  "contributors": [
    {
      "name": "Eric Gelinas",
      "email": "eric@standardpixel.com"
    }
  ],
  "main": "./server/index.js",
  "scripts": {
    "start:prod": "npm run webpack:prod && heroku local -f Procfile",
    "start:dev": "RELOAD_PORT=3001 npm run webpack:dev && heroku local -f Procfile",
    "webpack:dev": "rm -rf public/bundle.js && webpack --config ./webpack.config.dev.js",
    "webpack:prod": "rm -rf public/bundle.js && webpack --config ./webpack.config.prod.js",
    "brow": "webpack -t reactify -t require-globify public/index.js -o public/bundle.js",
    "pull": "git pull origin parksforward-redesign",
    "push": "git push origin parksforward-redesign",
    "heroku": "git push heroku parksforward-redesign:master",
    "postinstall": "npm run webpack:prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stamen/caliparks.org/tree/parksforward-redesign"
  },
  "bugs": {
    "url": "https://github.com/stamen/caliparks.org/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.1 <5",
    "npm": ">=3.1 <4"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.20.2",
    "es6-promise": "^3.0.2",
    "eslint": "^1.7.2",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "json-loader": "^0.5.3",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.3.3",
    "pg": "^4.4.3",
    "react": "^0.13.3",
    "react-engine": "^2.4.0",
    "react-router": "^0.13.4",
    "react-slick": "^0.8.1",
    "sass-loader": "^3.0.0",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
    "superagent": "^1.4.0",
    "webpack": "^1.10.5"
  },
  "devDependencies": {
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.12.1"
  }
